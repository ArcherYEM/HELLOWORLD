<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.core.tjoeun.mnHome.profile.sql.ProfileMapper">
	<insert id="addProfileHistory" parameterType="map">
		insert into profile (
			userNickname
			,image
			,msg
		) values(
			#{userNickname}
			,#{image}
			,#{msg}
		)
	</insert>	
</mapper>